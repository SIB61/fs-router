{
  "name": "@fs-router/koa",
  "version": "0.0.8",
  "publishConfig": {
    "access": "public",
    "dependencies": {
      "@fs-router/core": "^0.0.7"
    }
  },
  "description": "File-based routing for Koa",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sib61/fs-router.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "routing",
    "file-based",
    "koa"
  ],
  "license": "MIT",
  "dependencies": {
    "@fs-router/core": "workspace:*"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.5",
    "@types/node": "^22.10.5",
    "koa": "^2.15.3",
    "@koa/router": "^13.1.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@koa/router": "^12.0.0 || ^13.0.0",
    "koa": "^2.0.0"
  }
}
