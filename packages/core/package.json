{
  "name": "@fs-router/core",
  "version": "0.0.9",
  "publishConfig": {
    "access": "public"
  },
  "description": "Core parser and adapter interfaces for file-based routing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sib61/fs-router.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "routing",
    "file-based",
    "parser",
    "adapter"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^2.1.8",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
