{
  "name": "universal-fs-router",
  "version": "0.0.1",
  "description": "File-based routing for all JS/TS runtimes and frameworks",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./hono": {
      "types": "./dist/hono.d.ts",
      "import": "./dist/hono.mjs",
      "require": "./dist/hono.js"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "import": "./dist/express.mjs",
      "require": "./dist/express.js"
    },
    "./fastify": {
      "types": "./dist/fastify.d.ts",
      "import": "./dist/fastify.mjs",
      "require": "./dist/fastify.js"
    },
    "./koa": {
      "types": "./dist/koa.d.ts",
      "import": "./dist/koa.mjs",
      "require": "./dist/koa.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && node scripts/fix-imports.js",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "test:parser": "tsx scripts/test-parser.ts"
  },
  "keywords": [
    "routing",
    "file-based",
    "hono",
    "express",
    "fastify",
    "koa"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.5",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^2.1.8",
    "express": "^4.21.2",
    "fastify": "^5.2.2",
    "hono": "^4.6.14",
    "koa": "^2.15.3",
    "@koa/router": "^13.1.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@koa/router": "^12.0.0 || ^13.0.0",
    "express": "^4.0.0",
    "fastify": "^5.0.0",
    "hono": "^4.0.0",
    "koa": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "hono": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "koa": {
      "optional": true
    },
    "@koa/router": {
      "optional": true
    }
  }
}
