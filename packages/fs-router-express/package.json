{
  "name": "@fs-router/express",
  "version": "0.0.5",
  "publishConfig": {
    "access": "public",
    "dependencies": {
      "@fs-router/core": "^0.0.4"
    }
  },
  "description": "File-based routing for Express",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "routing",
    "file-based",
    "express"
  ],
  "license": "MIT",
  "dependencies": {
    "@fs-router/core": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.5",
    "express": "^4.21.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  }
}