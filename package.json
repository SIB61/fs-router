{
  "name": "universal-fs-router",
  "version": "0.0.7",
  "private": true,
  "description": "File-based routing for all JS/TS runtimes and frameworks",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck",
    "test:express": "pnpm --filter test-express dev",
    "test:hono": "pnpm --filter test-hono dev",
    "test:fastify": "pnpm --filter test-fastify dev",
    "test:koa": "pnpm --filter test-koa dev",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish"
  },
  "keywords": [
    "routing",
    "file-based",
    "hono",
    "express",
    "fastify",
    "koa"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "typescript": "^5.7.2"
  },
  "pnpm": {
    "overrides": {
      "is-generator-function": "1.0.10"
    }
  }
}
